<%
  const headerFooterJsFiles = [].filter.call(htmlWebpackPlugin.files.js, function(file){
    return file.indexOf('config') >= 0;
  });
%>

<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0"
/>
<title>%EC_BLURB_629149%</title>

<!-- css start -->
<% if(process.env.HAS_HEADER_FOOTER) { %>
  <link rel="stylesheet" type="text/css" href="%EC_CACHE_SERVER%/_shared/headerfooter/%EC_HEADER_FOOTER%/css/main.min.css"
/>
<% } %>

<% htmlWebpackPlugin.files.css && [].forEach.call(htmlWebpackPlugin.files.css, function(file) { %>
  <link rel="stylesheet" type="text/css" href="<%= file %>"></link>
<% }) %>
<!-- css end -->

<% if(process.env.HAS_HEADER_FOOTER) { %>
  <%- include config %>
  <% headerFooterJsFiles.forEach(function(file){ %>
    <script src="<%= file %>" type="text/javascript"></script>
  <% }) %>
<% } %>
